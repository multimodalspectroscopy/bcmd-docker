title = {{Shunting versus inactivation: analysis of presynaptic inhibitory mechanisms in primary afferents of the crayfish}},
journal = {Journal of Neuroscience},
year = {1999},
volume = {19},
number = {14},
pages = {6079--6089},
month = jul,
affiliation = {Laboratoire Neurobiologie et Mouvements, Centre National de la Recherche Scientifique, 13402 Marseille Cedex 20, France.},
