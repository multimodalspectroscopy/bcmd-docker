@article{AbdulGhani:1996vb,
author = {Abdul-Ghani, Muhammad A and Valiante, Taufik A and Pennefather, Peter S},
title = {{Sr2+ and quantal events at excitatory synapses between mouse hippocampal neurons in culture.}},
journal = {Journal of Physiology},
year = {1996},
volume = {495 ( Pt 1)},
pages = {113--125},
month = aug,
affiliation = {MRC Nerve Cell and Synapse Group, Faculty of Pharmacy, University of Toronto, Ontario, Canada.},
keywords = {Analysis, Ephys, Hippo, Patch Clamp, Template},
