file = {{J_Physiol_1983_Dingledine.pdf:/Users/matt/Dropbox/Library.papers3/Articles/1983/Dingledine/J_Physiol_1983_Dingledine.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/8B8240FC-27C7-474D-B0A6-ADC36E942AF9}}
}

