@article{Bradke:2000wf,
author = {Bradke, Frank and Dotti, Carlos G},
title = {{Differentiated neurons retain the capacity to generate axons from dendrites}},
journal = {Current Biology},
year = {2000},
volume = {10},
number = {22},
pages = {1467--1470},
month = nov,
affiliation = {Department of Anatomy, Howard Hughes Medical Institute, University of California, San Francisco, 513 Parnassus Avenue, California 94143-0452, USA. bradke@itsa.ucsf.edu},
