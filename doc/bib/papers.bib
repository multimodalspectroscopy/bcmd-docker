number = {9},
pages = {565--574},
month = nov,
affiliation = {Department of Medical Physics and Engineering, Leeds Teaching Hospitals, St James's University Hospital, Beckett Street, LS9 7TF, Leeds, UK. s.w.smye@leeds.ac.uk},
keywords = {Modelling},
