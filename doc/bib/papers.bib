file = {{1AB23509-1DD3-47B3-8463-8CA8719963EF.pdf:/Users/matt/Dropbox/Library.papers3/Files/1A/1AB23509-1DD3-47B3-8463-8CA8719963EF.pdf:application/pdf;1AB23509-1DD3-47B3-8463-8CA8719963EF.pdf:/Users/matt/Dropbox/Library.papers3/Files/1A/1AB23509-1DD3-47B3-8463-8CA8719963EF.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.3171/JNS.2008.109.12.1075}}
}

