@article{Letelier:2000tl,
author = {Letelier, Juan Carlos and Weber, Pamela P},
title = {{Spike sorting based on discrete wavelet transform coefficients.}},
journal = {Journal of Neuroscience Methods},
year = {2000},
volume = {101},
number = {2},
pages = {93--106},
month = sep,
affiliation = {Departamento de Biolog{\'\i}a, Facultad de Ciencias, Universidad de Chile, Casilla 653, Santiago, Chile. letelier@uchile.cl},
keywords = {Classification, Ephys, Wavelets},
