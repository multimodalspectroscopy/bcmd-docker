file = {{J_Physiol_1988_Ascher-1.pdf:/Users/matt/Dropbox/Library.papers3/Articles/1988/Ascher/J_Physiol_1988_Ascher-1.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/275482FC-C1D5-4E7E-B4E5-F093E49B1BD9}}
