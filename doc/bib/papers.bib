pages = {9104--9112},
month = dec,
affiliation = {Arbeitsgruppe Zellul{\"a}re Neurobiologie, Max-Planck-Institut f{\"u}r Biophysikalische Chemie, D-37077 G{\"o}ttingen, Germany.},
