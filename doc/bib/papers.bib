@article{Halabisky:2000wr,
author = {Halabisky, Brian and Friedman, Daniel and Radojicic, Milan and Strowbridge, Ben W},
title = {{Calcium influx through NMDA receptors directly evokes GABA release in olfactory bulb granule cells}},
journal = {Journal of Neuroscience},
year = {2000},
volume = {20},
number = {13},
pages = {5124--5134},
month = jul,
affiliation = {Department of Neurosciences, Case Western Reserve University, Cleveland, Ohio 44106-4975, USA.},
keywords = {NMDA},
