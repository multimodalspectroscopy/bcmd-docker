@article{Meir:1999vo,
author = {Meir, Alon and Ginsburg, Simona and Butkevich, Alexander and Kachalsky, Sylvia G and Kaiserman, Igor and Ahdut, Ronit and Demirgoren, Serdar and Rahamimoff, Rami},
title = {{Ion channels in presynaptic nerve terminals and control of transmitter release}},
