file = {{Smart-1987-J_Physiol.pdf:/Users/matt/Dropbox/Library.papers3/Articles/Smart/Smart-1987-J_Physiol.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/CDE13CDB-1506-473B-9D4F-C8DCBFC3228E}}
}

