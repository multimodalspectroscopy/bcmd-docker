@article{Qin:1996cl,
author = {Qin, Feng and Auerbach, Anthony and Sachs, Frederick},
title = {{Estimating single-channel kinetic parameters from idealized patch-clamp data containing missed events}},
journal = {Biophysical Journal},
year = {1996},
volume = {70},
number = {1},
pages = {264--280},
month = jan,
affiliation = {Department of Biophysical Sciences, State University of New York at Buffalo, New York 14214, USA. qin@acsu.buffalo.edu},
keywords = {Analysis, Patch Clamp},
doi = {10.1016/S0006-3495(96)79568-1},
pmid = {8770203},
pmcid = {PMC1224925},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-06-23T10:28:18GMT},
date-modified = {2014-12-18T13:22:52GMT},
abstract = {We present here a maximal likelihood algorithm for estimating single-channel kinetic parameters from idealized patch-clamp data. The algorithm takes into account missed events caused by limited time resolution of the recording system. Assuming a fixed dead time, we derive an explicit expression for the corrected transition rate matrix by generalizing the theory of Roux and Sauve (1985, Biophys. J. 48:149-158) to the case of multiple conductance levels. We use a variable metric optimizer with analytical derivatives for rapidly maximizing the likelihood. The algorithm is applicable to data containing substates and multiple identical or nonidentical channels. It allows multiple data sets obtained under different experimental conditions, e.g., concentration, voltage, and force, to be fit simultaneously. It also permits a variety of constraints on rate constants and provides standard errors for all estimates of model parameters. The algorithm has been tested extensively on a variety of kinetic models with both simulated and experimental data. It is very efficient and robust; rate constants for a multistate model can often be extracted in a processing time of approximately 1 min, largely independent of the starting values.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=8770203&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/matt/Dropbox/Library.papers3/Articles/1996/Qin/Biophysical_Journal_1996_Qin.pdf},
file = {{Biophysical_Journal_1996_Qin.pdf:/Users/matt/Dropbox/Library.papers3/Articles/1996/Qin/Biophysical_Journal_1996_Qin.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/S0006-3495(96)79568-1}}
}

