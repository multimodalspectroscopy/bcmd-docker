volume = {13},
number = {1},
pages = {80--90},
month = feb,
affiliation = {Institut f{\"u}r Mathematik und Wissenschaftliches Rechnen, Karl-Franzens-Universit{\"a}t Graz, Heinrichstrasse 36, 8010 Graz, Austria. stephen.keeling@uni-graz.at},
