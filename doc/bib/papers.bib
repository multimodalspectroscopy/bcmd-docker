@article{Bathori:1999uq,
author = {B{\`a}thori, Gy{\"o}rgy and Parolini, Isabella and Tombola, Francesco and Szab{\`o}, Ildik{\`o} and Messina, Angela and Oliva, Marta and De Pinto, Vito and Lisanti, Michael and Sargiacomo, Massimo and Zoratti, Mario},
title = {{Porin is present in the plasma membrane where it is concentrated in caveolae and caveolae-related domains}},
journal = {The Journal of biological chemistry},
year = {1999},
volume = {274},
number = {42},
pages = {29607--29612},
month = oct,
affiliation = {CNR Centro Studi Biomembrane, Dipartimento Scienze Biomediche Sperimentali, viale G. Colombo 3, 35121 Padova, Italy.},
keywords = {Maxi-Anion},
pmid = {10514428},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-11-07T17:31:37GMT},
date-modified = {2014-12-18T13:22:52GMT},
abstract = {Mitochondrial porin, or voltage-dependent anion channel, is a pore-forming protein first discovered in the outer mitochondrial membrane. Later investigations have provided indications for its presence also in other cellular membranes, including the plasma membrane, and in caveolae. This extra-mitochondrial localization is debated and no clear-cut conclusion has been reached up to now. In this work, we used biochemical and electrophysiological techniques to detect and characterize porin within isolated caveolae and caveolae-like domains (low density Triton-insoluble fractions). A new procedure was used to isolate porin from plasma membrane. The outer surface of cultured CEM cells was biotinylated by an impermeable reagent. Low density Triton-insoluble fractions were prepared from the labeled cells and used as starting material to purify a biotinylated protein with the same electrophoretic mobility and immunoreactivity of mitochondrial porin. In planar bilayers, the porin from these sources formed slightly anion-selective pores with properties indistinguishable from those of mitochondrial porin. This work thus provides a strong indication of the presence of porin in the plasma membrane, and specifically in caveolae and caveolae-like domains.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10514428&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/matt/Dropbox/Library.papers3/Articles/1999/Bathori/J._Biol._Chem._1999_Bathori.pdf},
file = {{J._Biol._Chem._1999_Bathori.pdf:/Users/matt/Dropbox/Library.papers3/Articles/1999/Bathori/J._Biol._Chem._1999_Bathori.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/ADBB1C89-0789-4EDA-9358-3B008E4097AF}}
}

