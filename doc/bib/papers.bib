file = {{J_Physiol_1987_Mayer.pdf:/Users/matt/Dropbox/Library.papers3/Articles/1987/Mayer/J_Physiol_1987_Mayer.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/A7B06309-C685-4B5D-8F68-2A1F057FF482}}
}

