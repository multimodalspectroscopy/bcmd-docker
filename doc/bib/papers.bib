journal = {Biophysical Journal},
year = {2000},
volume = {79},
number = {4},
pages = {1928--1944},
month = oct,
affiliation = {Department of Physiology and Biophysical Sciences, State University of New York at Buffalo, Buffalo, New York 14214, USA. qin@acsu.buffalo.edu},
keywords = {Analysis, HMM, Patch Clamp},
