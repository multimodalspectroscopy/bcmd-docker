@article{Transtrum:2012kf,
author = {Transtrum, Mark K and Qiu, Peng},
title = {{Optimal experiment selection for parameter estimation in biological differential equation models.}},
journal = {BMC Bioinformatics},
year = {2012},
volume = {13},
pages = {181},
affiliation = {Department of Bioinformatics and Computational Biology, University of Texas M,D, Anderson Cancer Cneter, Houston, Texas, USA.},
keywords = {Modelling},
doi = {10.1186/1471-2105-13-181},
pmid = {22838836},
pmcid = {PMC3536579},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-05-30T11:20:04GMT},
date-modified = {2014-12-18T13:22:52GMT},
abstract = {BACKGROUND:Parameter estimation in biological models is a common yet challenging problem. In this work we explore the problem for gene regulatory networks modeled by differential equations with unknown parameters, such as decay rates, reaction rates, Michaelis-Menten constants, and Hill coefficients. We explore the question to what extent parameters can be efficiently estimated by appropriate experimental selection.

RESULTS:A minimization formulation is used to find the parameter values that best fit the experiment data. When the data is insufficient, the minimization problem often has many local minima that fit the data reasonably well. We show that selecting a new experiment based on the local Fisher Information of one local minimum generates additional data that allows one to successfully discriminate among the many local minima. The parameters can be estimated to high accuracy by iteratively performing minimization and experiment selection. We show that the experiment choices are roughly independent of which local minima is used to calculate the local Fisher Information.

CONCLUSIONS:We show that by an appropriate choice of experiments, one can, in principle, efficiently and accurately estimate all the parameters of gene regulatory network. In addition, we demonstrate that appropriate experiment selection can also allow one to restrict model predictions without constraining the parameters using many fewer experiments. We suggest that predicting model behaviors and inferring parameters represent two different approaches to model calibration with different requirements on data and experimental cost.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22838836&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/matt/Dropbox/Library.papers3/Articles/2012/Transtrum/BMC_Bioinformatics_2012_Transtrum.pdf},
file = {{BMC_Bioinformatics_2012_Transtrum.pdf:/Users/matt/Dropbox/Library.papers3/Articles/2012/Transtrum/BMC_Bioinformatics_2012_Transtrum.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1186/1471-2105-13-181}}
}

