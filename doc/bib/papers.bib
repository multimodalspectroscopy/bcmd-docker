@article{Kind:2010fu,
author = {Kind, Taco and Faes, Theo J C and Lankhaar, Jan-Willem and Vonk-Noordegraaf, Anton and Verhaegen, Michel},
title = {{Estimation of three- and four-element windkessel parameters using subspace model identification.}},
journal = {IEEE Transactions on Biomedical Engineering},
year = {2010},
