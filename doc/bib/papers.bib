@article{Stuart:1997vw,
author = {Stuart, Greg J and Schiller, Jackie and Sakmann, Bert},
title = {{Action potential initiation and propagation in rat neocortical pyramidal neurons.}},
journal = {Journal of Physiology},
year = {1997},
volume = {505 ( Pt 3)},
pages = {617--632},
month = dec,
affiliation = {Abteilung Zellphysiologie, Max-Planck-Institut f{\"u}r medizinische Forschung, Heidelberg, Germany. Greg.Stuart@anu.edu.au},
