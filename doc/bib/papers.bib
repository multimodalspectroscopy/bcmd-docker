@article{Brown:2004kt,
author = {Brown, K S and Hill, C C and Calero, G A and Myers, C R and Lee, K H and Sethna, James P and Cerione, R A},
title = {{The statistical mechanics of complex signaling networks: nerve growth factor signaling}},
journal = {Physical Biology},
year = {2004},
volume = {1},
