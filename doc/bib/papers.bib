@article{Ying:2004vc,
author = {Ying, Liming and White, Samuel S and Bruckbauer, Andreas and Meadows, Lisa and Korchev, Yuri E and Klenerman, David},
title = {{Frequency and voltage dependence of the dielectrophoretic trapping of short lengths of DNA and dCTP in a nanopipette}},
journal = {Biophysical Journal},
year = {2004},
volume = {86},
