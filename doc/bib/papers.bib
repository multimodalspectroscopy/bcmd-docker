year = {1996},
volume = {497 ( Pt 2)},
pages = {531--537},
month = dec,
affiliation = {Arbeitsgruppe Zellul{\"a}re Neurobiologie, Max-Planck-Institut f{\"u}r Biophysikalische Chemie, G{\"o}ttingen, Germany.},
